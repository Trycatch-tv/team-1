<main class="page">
  <section class="contact">
    <h2>Registro de empleado nuevo</h2>
    <h3>Datos de ingreso</h3>
    <form action="">

      <div class="form-grupo">
        <label for="cargo">Cargo</label>
        <input type="text" name="cargo" id="cargo" required />
      </div>

      <div class="completeDoubleInputLine">

        <div class="form-grupo dobleInput">
          <label for="project">Proyecto</label>
          <input type="text" name="project" id="project" required />
        </div>
      </div>
      <div class="completeDoubleInputLine">
        <div class="form-grupo dobleInput">
          <label for="ingreso">Fecha de ingreso</label>
          <input type="date" name="ingreso" id="ingreso" required />
        </div>

        <div class="form-grupo dobleInput">
          <label for="sueldo">Sueldo</label>
          <input type="text" name="sueldo" id="sueldo" required />
        </div>
      </div>
      <div class="button">
        <button type="submit">Registrar</button>
      </div>
    </form>

  </section>
</main>