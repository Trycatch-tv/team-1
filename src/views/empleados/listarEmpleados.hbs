<div class="container">
  <h2>Lista de empleados </h2>
  <div class="container">
    <div class="row pt-5">
      <div class="col-md-5">
        <div class="card">
          <div class="card-body">
            <table class="table">
              <thead>
                <tr>
                  <th scope="col">#</th>
                  <th scope="col">Name</th>
                </tr>
              </thead>
              <tbody>
                {{#each data}}
                <tr>
                  <td scope="row">{{this.id}}</td>
                  <td scope="row">{{this.nombre}}</td>
                  {{else}}
                  <td td scope="row">Sin datos</td>
                  {{/each}}
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  

</div>