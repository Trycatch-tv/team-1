<main class="page">
  <section class="contact">
    <h2>Registro de nuevo proyecto</h2>

    <form action="/proyecto/ingresodatos" method="post">

      <div class="form-grupo dobleInput">
        <label for="identificacion">Identificación</label>
        <input
          type="text"
          name="id_empleado"
          id="identificacion"
          required
          autofocus
        />
      </div>

      <div class="form-grupo dobleInput">
        <label for="proyectName">Nombre del proyecto</label>
        <input type="text" name="nombre" id="proyectName" required />
      </div>

      <div class="form-grupo dobleInput">
        <label for="projectDescription">Descripcion del proyecto</label>
        <input
          type="text"
          name="descripcion"
          id="projectDescription"
          required
        />
      </div>

      <div class="completeDoubleInputLine">
        <div class="form-grupo dobleInput">
          <label for="initDate">Fecha de inicio</label>
          <input type="date" name="fecha_inicio" id="initDate" required />
        </div>
        <div class="form-grupo dobleInput">
          <label for="endDate">Fecha de finalización</label>
          <input type="date" name="fecha_final" id="endDate" required />
        </div>
      </div>


      <div class="button">
        <button type="submit" class="btn btn-primary">Registrar</button>
      </div>
    </form>

  </section>
</main>